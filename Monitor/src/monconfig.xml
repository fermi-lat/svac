<monitoring>

  <inputList name="top"> 
    <object> nACD </object>
    <object> Gem_elapsed </object>
    <object> timestamp </object>
    <object> condsummary </object>
  </inputList>


  <outputList name="top">
    <object>
      <name> nAcd  </name>
      <formula><![CDATA[   nACD ]]></formula>
      <cut><![CDATA[ 1 ]]></cut>
      <type> mean </type>
    </object>
    <object>
      <name> Gem_elapsed  </name>
      <formula><![CDATA[   Gem_elapsed ]]></formula>
      <cut><![CDATA[ 1 ]]></cut>
      <type> counterdiff </type>
    </object>
    <object>
      <name> alltriggers  </name>
      <formula><![CDATA[ 1 ]]></formula>
      <cut><![CDATA[ condsummary==7 ]]></cut>
      <type> counter </type>
    </object>
    <object>
      <name> engine2 </name>
      <formula><![CDATA[ 1 ]]></formula>
      <cut><![CDATA[ RFun::engine(condsummary)==7 ]]></cut>
      <type> counter </type>
    </object>
    <object>
      <name> engine[16] </name>
      <formula><![CDATA[ (RFun::engine(condsummary)==engine) ]]></formula>
      <cut><![CDATA[ foreachengine ]]></cut>
      <type> counter </type>
    </object>

  </outputList>

  <eventCut><![CDATA[ condsummary&4  ]]></eventCut>
  <timestamp> timestamp </timestamp>
</monitoring>
