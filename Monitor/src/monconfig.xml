<monitoring>

  <inputList name="top"> 
    <object> nACD </object>
    <object> Gem_elapsed </object>
    <object> timestamp </object>
    <object> condsummary </object>
    <object> configkey1852 </object>
  </inputList>


  <outputList name="top">
    <object>
      <name> nAcd  </name>
      <formula><![CDATA[   nACD ]]></formula>
      <cut><![CDATA[ nACD>2 ]]></cut>
      <type> mean </type>
    </object>
    <object>
      <name> Gem_elapsed  </name>
      <formula><![CDATA[   Gem_elapsed ]]></formula>
      <cut><![CDATA[ 1 ]]></cut>
      <type> counterdiff </type>
    </object>
    <object>
      <name> configkey </name>
      <formula><![CDATA[ configkey1852 ]]></formula>
      <cut><![CDATA[  ]]></cut>
      <type> minmax </type>
    </object>
    <object>
      <name> engine0 </name>
      <formula><![CDATA[   RFun::iden( nACD ) +1 ]]></formula>
      <cut><![CDATA[ nACD> 1  ]]></cut>
      <type> mean </type>
    </object>
    <object>
      <name> engine[16] </name>
      <formula><![CDATA[foreachengine (RFun::engine(condsummary,configkey1852)==engine) ]]></formula>
      <cut><![CDATA[  ]]></cut>
      <type> counter </type>
    </object>

  </outputList>

  <eventCut><![CDATA[ nACD>4  ]]></eventCut>
  <timestamp> timestamp </timestamp>
</monitoring>
