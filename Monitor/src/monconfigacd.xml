<monitoring>

  <inputList name="top"> 

    <!-- ACD variables -->
    
    <!-- Time stamp -->
    <object>timestamp</object>

    <!-- Acd Variables -->
    <object>acdpha</object>
    <object>condsummary</object>

    <!-- Gem Variables -->
    <object>numvetotiles</object>

  </inputList>


  <outputList name="top">
    <!-- TEST variables -->
    <object>
      <name> ped[12][18]  </name>
      <formula><![CDATA[ foreachgarc:foreachgafe:(RFun::acdped(timestamp,garc,gafe)>1)*(acdpha[garc][gafe]-RFun::acdped(timestamp,garc,gafe)) ]]></formula>
      <cut><![CDATA[ condsummary&32 ]]></cut>
      <type> truncatedmeanfrac[.9] </type>
    </object>
    <object>
      <name> ndig[12][18]  </name>
      <formula><![CDATA[ acdpha>0 ]]></formula>
      <cut><![CDATA[ !(condsummary&32) ]]></cut>
      <type> counter </type>
    </object>
    <object>
      <name> numveto  </name>
      <formula><![CDATA[ numvetotiles>0 ]]></formula>
      <cut><![CDATA[ ]]></cut>
      <type> counter </type>
    </object>
    <object>
      <name> numnonper  </name>
      <formula><![CDATA[ 1 ]]></formula>
      <cut><![CDATA[ !(condsummary&32) ]]></cut>
      <type> counter </type>
    </object>
    <object>
      <name> numperiodic  </name>
      <formula><![CDATA[ 1  ]]></formula>
      <cut><![CDATA[ condsummary&32 ]]></cut>
      <type> counter </type>
    </object>
    <object>
      <name> timeinterval  </name>
      <formula><![CDATA[ timestamp  ]]></formula>
      <cut><![CDATA[  ]]></cut>
      <type> counterdiff </type>
    </object>
    <object>
      <name> phaover30[12][18]  </name>
      <formula><![CDATA[ foreachgarc:foreachgafe:RFun::acdped(timestamp,garc,gafe)>0 && (acdpha[garc][gafe]-RFun::acdped(timestamp,garc,gafe)>30)]]></formula>
      <cut><![CDATA[ condsummary&32 ]]></cut>
      <type> counter </type>
    </object>
    <object>
      <name> pedhist[12][18]  </name>
      <formula><![CDATA[ ped ]]></formula>
      <cut><![CDATA[  ]]></cut>
      <source>output</source>
      <type> histogram-1d[100,-50,50] </type>
    </object>
    <object>
      <name> phaover30rateall  </name>
      <formula><![CDATA[ Sum$(phaover30)/numperiodic ]]></formula>
      <cut><![CDATA[  ]]></cut>
      <source>output</source>
      <type> histogram-1d[100,0,1] </type>
    </object>
    <object>
      <name> phaover30rate[12][18]  </name>
      <formula><![CDATA[ phaover30/numperiodic ]]></formula>
      <cut><![CDATA[  ]]></cut>
      <source>output</source>
      <type> histogram-1d[100,0,.02] </type>
    </object>
    <object>
      <name> numdigi[12][18]  </name>
      <formula><![CDATA[ ndig/numnonper ]]></formula>
      <cut><![CDATA[  ]]></cut>
      <source>output</source>
      <type> histogram-1d[100,0,.1] </type>
    </object>
    <object>
      <name> numdigiall  </name>
      <formula><![CDATA[ Sum$(ndig)/numnonper ]]></formula>
      <cut><![CDATA[  ]]></cut>
      <source>output</source>
      <type> histogram-1d[100,0,10] </type>
    </object>
    <object>
      <name> vetorate  </name>
      <formula><![CDATA[ numveto/timeinterval ]]></formula>
      <cut><![CDATA[  ]]></cut>
      <source>output</source>
      <type> histogram-1d[100,1,1000] </type>
    </object>
    
    
    
  </outputList>

  <eventCut><![CDATA[ ]]></eventCut>
  <timestamp> timestamp </timestamp>
</monitoring>
