    <!-- RAtes -->
    <object>
      <name> EvtsBeforeCuts </name>
      <formula><![CDATA[ 1 ]]></formula>
      <cut><![CDATA[ ]]></cut>
      <type> rate </type>
      <description> Rate of events before applicaltion of cuts.</description>
    </object>

     <object>
      <name> EvtsBeforeCutsBelow105ZenithTheta </name>
      <formula><![CDATA[ 1 ]]></formula>
      <cut><![CDATA[ FT1ZenithTheta<105 ]]></cut>
      <type> rate </type>
      <description> Rate of events before application of gamma selection cuts that survive the cut FT1ZenithTheta smaller than 105.</description>
    </object>

     <object>
      <name> EvtsBeforeCutsAbove105ZenithTheta </name>
      <formula><![CDATA[ 1 ]]></formula>
      <cut><![CDATA[ FT1ZenithTheta>105 ]]></cut>
      <type> rate </type>
      <description> Rate of events before application of gamma selection cuts that survive the cut FT1ZenithTheta larger than 105.</description>
    </object>

     <object>
      <name> TransientEvts </name>
      <formula><![CDATA[ 1 ]]></formula>
      <cut><![CDATA[ (GltGemSummary&10)>0 && ((GltGemSummary&7)!=3) && (FilterStatus_HI==0) && CTBBestEnergyProb>0.1 && CTBCORE>0.1 && CTBBestEnergy>10 && CTBBestEnergyRatio<5 && (CTBClassLevel>0) ]]></cut>
      <type> rate </type>
      <description> Rate of events after application of TRANSIENT gamma cuts from pass5. </description>
    </object>

      <object>
      <name> DiffuseEvts </name>
      <formula><![CDATA[ 1 ]]></formula>
      <cut><![CDATA[ (GltGemSummary&10)>0 && ((GltGemSummary&7)!=3) && (FilterStatus_HI==0) && CTBBestEnergyProb>0.1 && CTBCORE>0.1 && CTBBestEnergy>10 && CTBBestEnergyRatio<5 && (CTBClassLevel>2) ]]></cut>
      <type> rate </type>
      <description> Rate of events after application of DIFFUSE gamma cuts from pass5. </description>
    </object>


       <object>
      <name> SourceEvts </name>
      <formula><![CDATA[ 1 ]]></formula>
      <cut><![CDATA[ (GltGemSummary&10)>0 && ((GltGemSummary&7)!=3) && (FilterStatus_HI==0) && CTBBestEnergyProb>0.1 && CTBCORE>0.1 && CTBBestEnergy>10 && CTBBestEnergyRatio<5 && (CTBClassLevel>1) ]]></cut>
      <type> rate </type>
      <description> Rate of events after application of SOURCE gamma cuts from pass5. </description>
    </object>



       <object>
      <name> TransientEvtsBelowZenithTheta105 </name>
      <formula><![CDATA[ 1 ]]></formula>
      <cut><![CDATA[ (GltGemSummary&10)>0 && ((GltGemSummary&7)!=3) && (FilterStatus_HI==0) && CTBBestEnergyProb>0.1 && CTBCORE>0.1 && CTBBestEnergy>10 && CTBBestEnergyRatio<5 && (CTBClassLevel>0) && FT1ZenithTheta<105 ]]></cut>
      <type> rate </type>
      <description> Rate of events after application of TRANSIENT gamma cuts from pass5 and the cut FT1ZenithTheta smaller than 105. </description>
    </object>

      <object>
      <name> DiffuseEvtsBelowZenithTheta105  </name>
      <formula><![CDATA[ 1 ]]></formula>
      <cut><![CDATA[ (GltGemSummary&10)>0 && ((GltGemSummary&7)!=3) && (FilterStatus_HI==0) && CTBBestEnergyProb>0.1 && CTBCORE>0.1 && CTBBestEnergy>10 && CTBBestEnergyRatio<5 && (CTBClassLevel>2) && FT1ZenithTheta<105 ]]></cut>
      <type> rate </type>
      <description> Rate of events after application of DIFFUSE gamma cuts from pass5 and the cut FT1ZenithTheta smaller than 105. </description>
    </object>


       <object>
